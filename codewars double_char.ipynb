   "source": [
    "# Double Char\n",
    "#Given a string, you have to return a string in which each character (case-sensitive) is repeated once."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "b308edbc",
   "metadata": {},
   "outputs": [],
   "source": [
    "def double_char(s):\n",
    "    return \"\".join([i*2 for i in s])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
